[user]
	name = jaborandi
	email = support@excsitement.com
[push]
	default = simple
[format]
	pretty = "%h %s [%ar]%x0D%x0A"
[alias]
    loga = log --pretty=format:\"%h [%ad %cd] %s\"

    re = reflog --pretty=reflog
	lg = "!f() { git log --reverse --ancestry-path $1^..HEAD | head $2; }; f"
	b = !git --git-dir=/.ecup --work-tree=/
	c = !git --git-dir=/.common --work-tree=/
[pretty]
	reflog = "%ar %h %gs"
[core]
	mergeoptions = --no-edit
	fileMode = false
	quotePath = false
	pager = 
[init]
	templatedir = "~/.git_template"
	defaultBranch = "master"
[remote]
	pushDefault = origin
[log]
	date = format:%d-%m-%y %H:%M
[merge]
	conflictstyle = merge
[safe]
	directory = *
[protocol "file"]
	allow = always
